#!/usr/bin/env node

// Do not limit stack trace
Error.stackTraceLimit = Infinity;

var insight = require('../lib/insight'),
    coa = require('../lib/coa');

if (insight.optOut === undefined) {
    insight.track('first-run');
    insight.askPermission(null, run);
} else {
    run();
}

function run() {
    coa.run();
}
